{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",

  "required": ["name", "age", "subjects"],

  "properties": {
    "name": {
      "type": "string"
    },
    "age": {
      "type": "integer"
    },
    "subjects": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}
